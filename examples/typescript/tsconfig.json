{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "outDir": "./build",
    "strict": true,
    "baseUrl": "./src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*"]
}
