const { auth } = require('@tensei/auth')
const { tensei } = require('@tensei/core')
const { graphql } = require('@tensei/graphql')

tensei()
    .root(__dirname)
    .plugins([
        auth()
            .jwt()
            .plugin(),
        graphql()
            .plugin()
    ])
    .start()
    .catch(console.error)
