{
    "name": "@tensei/rest",
    "version": "0.8.9",
    "main": "./build/index.js",
    "license": "MIT",
    "types": "./build/index.d.ts",
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/jest": "^25.2.3",
        "@types/supertest": "^2.0.9",
        "autoprefixer": "^10.2.4",
        "jest": "^26.0.1",
        "laravel-mix": "^6.0.13",
        "postcss": "^8.2.7",
        "prettier": "^2.0.5",
        "supertest": "^4.0.2",
        "typescript": "^4.2.4"
    },
    "scripts": {
        "prettier": "prettier --write './**/*.{js,json,ts,css,tsx}'",
        "build": "rimraf build && yarn build:server && yarn build:client && yarn copy:files",
        "dev:server": "tsc --watch --p tsconfig.server.json",
        "copy:files": "copyfiles -u 1 src/docs/index.mustache build/",
        "dev:client": "cross-env NODE_ENV=development webpack --config=node_modules/laravel-mix/setup/webpack.config.js --watch",
        "test": "jest --verbose --runInBand --forceExit",
        "build:server": "tsc --p tsconfig.server.json",
        "build:client": "cross-env NODE_ENV=production webpack --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "dependencies": {
        "@tensei/common": "^0.8.9",
        "@tensei/core": "^0.8.9",
        "express-response-formatter": "^2.0.2",
        "pluralize": "^8.0.0",
        "qs": "^6.9.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "5f6d71bf6b43eb8543b92db59bddc5535202b590"
