{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "./build/docs",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es5",
        "baseUrl": "./src/docs",
        "esModuleInterop": true,
        "declaration": false
    },
    "exclude": ["__tests__", "build", "src/index.ts", "src/populate-helpers.ts"]
}
