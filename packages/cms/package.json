{
    "name": "@tensei/cms",
    "version": "0.7.10",
    "main": "./build/index.js",
    "license": "MIT",
    "types": "./build/index.d.ts",
    "files": [
        "build/",
        "default-assets/"
    ],
    "scripts": {
        "prettier": "prettier --write './**/*.{js,json,ts,tsx,css}'",
        "dev:css": "cross-env NODE_ENV=development yarn build:css",
        "dev:server": "tsc --watch --project tsconfig.server.json",
        "build:server": "tsc --project tsconfig.server.json && yarn copy:files",
        "copy:files": "copyfiles -u 1 plugin/template/index.mustache build/",
        "dev": "webpack --config=node_modules/laravel-mix/setup/webpack.config.js --watch",
        "build": "yarn build:server && cross-env NODE_ENV=production webpack --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/preset-react": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "@reach/accordion": "^0.12.1",
        "@tailwindcss/aspect-ratio": "^0.2.0",
        "@tailwindcss/forms": "^0.2.1",
        "@tailwindcss/typography": "^0.3.1",
        "@tailwindcss/ui": "^0.7.2",
        "@tensei/components": "^0.7.10",
        "@types/axios": "^0.14.0",
        "@types/classnames": "^2.2.11",
        "@types/qs": "^6.9.5",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-gravatar": "^2.6.8",
        "@types/react-paginate": "^6.2.1",
        "@types/react-router-dom": "^5.1.6",
        "@types/react-select": "^3.1.2",
        "@types/speakingurl": "^13.0.2",
        "@types/throttle-debounce": "^2.1.0",
        "autoprefixer": "^10.2.4",
        "axios": "^0.21.1",
        "classnames": "^2.2.6",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "laravel-mix": "^6.0.5",
        "postcss": "^8.2.4",
        "qs": "^6.9.4",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-gravatar": "^2.6.3",
        "react-paginate": "^7.0.0",
        "react-router-dom": "^5.2.0",
        "react-select": "^3.1.1",
        "speakingurl": "^14.0.1",
        "tailwindcss": "^2.0.2",
        "throttle-debounce": "^3.0.1",
        "toastedjs": "^0.0.2",
        "ts-loader": "^8.0.11",
        "typescript": "^4.1.2"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@tensei/mail": "^0.7.10",
        "@types/csurf": "^1.11.0",
        "@types/express-session": "^1.17.3",
        "csurf": "^1.11.0",
        "express-session": "^1.17.1",
        "express-session-mikro-orm": "^0.7.10"
    }
}
