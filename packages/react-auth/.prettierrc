{
	"trailingComma": "all",
	"semi": false,
	"singleQuote": true,
	"useTabs": false,
	"quoteProps": "consistent",
	"bracketSpacing": true,
	"arrowParens": "always",
	"printWidth": 120,
	"tabWidth": 2,
	"jsxBracketSameLine": false,
	"proseWrap": "always",
	"prefer-template": 2,
	"no-var": 2,
	"react-hooks/exhaustive-deps": 0
}
